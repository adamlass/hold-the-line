    "contents": [
        {
            "text": "Welcome to RaiseMe, your San Francisco-based marketplace for earning college micro-scholarships beginning in ninth grade.",
            "fileID": "9ad7672696984e3aa58e42a28adb503a"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For Students and Parents, press 1.",
                "fileID": "a851eb89424e4af58b65b9bf59676384",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To inquire about signing up as a student, press 1.",
