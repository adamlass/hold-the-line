    "contents": [
        {
            "text": "Welcome to HackerOne in San Francisco, trusted by Fortune 500 and Forbes Global 1000 companies to secure their critical applications.",
            "fileID": "16feae479c9f405f8884fa012c6f1569"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are an existing customer seeking security support, press 1.",
