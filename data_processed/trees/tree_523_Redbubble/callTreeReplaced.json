                "index": 2,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "For help with uploading artwork and creating listings, press 1.",
                            "fileID": "53136d24cbc3420da3185ab32e835b3e",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
