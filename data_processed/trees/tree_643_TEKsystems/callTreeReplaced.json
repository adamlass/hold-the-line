                                ],
                                "nextSegment": null,
                                "id": "bf5b45c0ba1a489384636e3c37d13646",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To check the status of your application, press 2.",
                            "fileID": "faf07bc26cbe44d5864af1b834185d72",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
