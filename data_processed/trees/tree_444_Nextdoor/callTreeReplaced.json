        {
            "text": "Welcome to Nextdoor, the free private social network for your neighborhood community.",
            "fileID": "b778536b6bc44f3b8b41a672797e3708"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
