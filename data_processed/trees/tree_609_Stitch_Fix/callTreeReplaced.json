    "contents": [
        {
            "text": "Welcome to Stitch Fix, your inclusive personal styling service based in San Francisco.",
            "fileID": "0fab46c06d3f41bfb94f0dcacf3b3530"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
