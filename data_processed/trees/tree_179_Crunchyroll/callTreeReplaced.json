    "contents": [
        {
            "text": "Thank you for calling Crunchyroll, your San Francisco\u2013based destination for legal anime streaming and simulcasts.",
            "fileID": "239599ad965f48f7b3b27ecea789386f"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "To manage your subscription or account, press 1.",
                "fileID": "51ddf0b3726e43a9a9579bb2844a7dd2",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
