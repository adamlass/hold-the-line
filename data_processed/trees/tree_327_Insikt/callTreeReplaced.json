{
    "type": "welcome",
    "contents": [
        {
            "text": "Welcome to Insikt, San Francisco\u2019s trusted provider of affordable small loans for hardworking people with low or no credit.",
            "fileID": "fb9158fbf18549d3bfbc6a37da377937"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For new loan applications or to manage existing loans, press 1.",
                "fileID": "c5b71677e2c6473aa778436f76fd2ae0",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To apply for a new small loan today, press 1.",
                            "fileID": "b4b09f7280294b94850c59ebea5277ba",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to a loan specialist.",
                                        "fileID": "f41c9f4e52e44f519edd386bc25f2689"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "383daef7d40348e0bc9fe8f45a8cdd7a",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To check your loan balance or make a payment, press 2.",
                            "fileID": "df5eea98c6204bfcb11db8bf5f785363",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we retrieve your account information.",
                                        "fileID": "24a58ea5cfa844ed9acc717e8251f568"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "cc0c2bf44d31455093df28ba19a81ea0",
