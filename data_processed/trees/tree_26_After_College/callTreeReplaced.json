{
    "type": "welcome",
    "contents": [
        {
            "text": "Thank you for calling After College in San Francisco, connecting talented graduates directly with the employers who want to hire you.",
            "fileID": "31b89fcedb854805a74d1d28e90ea70f"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are an employer looking to recruit college talent, press 1.",
                "fileID": "3ad7fa294b6f43feb58b44aae92a8e34",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To post a new job listing on our marketplace, press 1.",
                            "fileID": "ac6de8b3aa2348228afabb1b13e7ef6d",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you with our job posting support team.",
                                        "fileID": "7472c0422d134900ac67733451398b70"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "b3eea5ce0330475f85e41cae275a80c1",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To learn about enterprise solutions and pricing, press 2.",
                            "fileID": "9bb944e484664c78a5fc22883f81e661",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
