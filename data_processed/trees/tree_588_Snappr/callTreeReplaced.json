    "contents": [
        {
            "text": "Welcome to Snappr in San Francisco, where great photography isn\u2019t so hard.",
            "fileID": "02b4f05a83ea435eb4909c609aef752b"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For new bookings or information on our photography packages, press 1.",
                "fileID": "e4a47a5da81f4e7181f67def6f7c8355",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To schedule a session with a Snappr photographer, press 1.",
                            "fileID": "008b44ccd86247d49a36b618a57dac8a",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to our scheduling team.",
                                        "fileID": "1b898bb88f6546ea926b18535a844291"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "2083675a5066492e8ab4d0cf931e6848",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To hear our photography package options and pricing, press 2.",
                            "fileID": "fe9a4bbde00e417ab90bd4cfca20b7e8",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we retrieve our package and pricing details for you.",
