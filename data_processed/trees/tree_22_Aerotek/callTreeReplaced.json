                                ],
                                "nextSegment": null,
                                "id": "96a8fd24bd7b45a3a5ee17f0fc2e20b1",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To learn how to submit your resume online, press 2.",
                            "fileID": "cf7591293b0f4518ad57af4f66a09266",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
