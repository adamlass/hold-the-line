    "contents": [
        {
            "text": "Welcome to OpenTable in San Francisco, where you can find your table for any occasion.",
            "fileID": "3ea8a25596c440b59efdece915d0ac0f"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "To make a new reservation, press '1'.",
                "fileID": "631d224e0c30492ca24c99b5978af662",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To reserve a table for today, press '1'.",
                            "fileID": "3bddbda203ec4a5caee167bb9c94cec2",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
