    "contents": [
        {
            "text": "Welcome to Binti, where we build software in the East Bay to match every child with the best foster family.",
            "fileID": "c8aad56a52a044c2ac3a3c834b954d9c"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
