    "contents": [
        {
            "text": "Welcome to TEKsystems in San Francisco, where we're building tomorrow's workforce today.",
            "fileID": "4614037aa103436e97ece6dd95fbd7e7"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are a job seeker looking to build your career, press 1.",
