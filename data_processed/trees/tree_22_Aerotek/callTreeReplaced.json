    "contents": [
        {
            "text": "Thank you for calling Aerotek San Francisco, the leading recruitment and staffing agency bringing great people and great organizations together.",
            "fileID": "fe545d707a9747e39fc2c29a774e17b4"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are a job seeker looking for new opportunities, press 1.",
                "fileID": "4ba89d291f5c405ea1eff3dd542d84b4",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
