    "contents": [
        {
            "text": "Welcome to Eventbrite in San Francisco, where you can find your next experience.",
            "fileID": "ab7b49a42586426a9044af680eae770d"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For ticket purchases and order inquiries, press 1.",
