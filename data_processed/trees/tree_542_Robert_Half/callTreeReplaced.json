        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For job seekers looking to start or advance your career, press 1.",
                "fileID": "b2f78d15d840431f97050ca08753a120",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
