    "contents": [
        {
            "text": "Thank you for calling Postman, the complete API development environment used by over six million developers worldwide.",
            "fileID": "e3f3d7dd6ec14fa8aed964a5a2bc8585"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For sales and account information, press 1.",
                "fileID": "71dc686f1de44687b5e92dc82b277b7c",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To set up a new Postman account, press 1.",
                            "fileID": "63c174a75c4b4aaea45d289e32ca7b13",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you with a sales specialist for new accounts. Thank you for your patience.",
                                        "fileID": "76eaa082841b47b49ece317c610f4d9a"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "48c5fee748ff4950829d6b89cb0b30b3",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "For billing, upgrades, or account management on existing plans, press 2.",
                            "fileID": "1bd88b9fea324951857926c55a69c03f",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you with our billing department. We appreciate your patience.",
