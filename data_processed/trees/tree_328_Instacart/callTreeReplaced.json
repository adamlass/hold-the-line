{
    "type": "welcome",
    "contents": [
        {
            "text": "Thank you for calling Instacart in San Francisco, delivering groceries in as little as one hour.",
            "fileID": "a876f82e3f9240e2ba20640b3cb85f3f"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "To place a new grocery delivery order, press 1.",
                "fileID": "f82b2eb93bba497898fdb36d109b0cf4",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To speak with a sales specialist about our delivery options, press 1.",
                            "fileID": "708c763e5ab54d088188e5d937598b53",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you with our sales team. Your call is important to us.",
                                        "fileID": "d1c337437014463685798e3b79cd4579"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "5a1bd46900f2463aa3732ffaad170a18",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To learn about current promotions and savings, press 2.",
                            "fileID": "df6a9325dc7c4da382e562fe0caeeb9e",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we retrieve the latest promotions. Thank you for your patience.",
                                        "fileID": "35e153de3dfd4c78afc20a6bbda93a1b"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "7f7b632e8b4142b7bfd8bdde85d47e92",
                                "navigationGoals": null
                            }
                        }
                    ],
                    "nextSegment": null,
                    "id": "3166c539b721447b9c1cd8790cf4dde7",
                    "navigationGoals": null
                }
            },
            {
                "text": "For order support and existing orders, press 2.",
                "fileID": "a79e21beee094428adbeabd7bdca24ec",
                "index": 2,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To track the status of your current order, press 1.",
                            "fileID": "6c5fcaef868d48098bb44e3ac7e307b9",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we look up your order status. We appreciate your patience.",
                                        "fileID": "4f16b530429346a2ab97084d77347ef8"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "1360a0f495f34668998ada1da59232a5",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To modify or cancel an existing order, press 2.",
                            "fileID": "e4c29d9a415e49b18dc1336837eb71e3",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we process your request. Please stay on the line.",
                                        "fileID": "c6427e835e074f75a244341b2c90fd5f"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "aa290303ac8647b987e39e0216a887e7",
                                "navigationGoals": null
                            }
                        }
                    ],
                    "nextSegment": null,
                    "id": "ece2a1d2fe7545de99845d2a44ebb512",
                    "navigationGoals": null
                }
            }
        ],
