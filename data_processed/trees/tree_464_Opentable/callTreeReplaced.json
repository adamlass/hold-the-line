                                    }
                                ],
                                "nextSegment": null,
                                "id": "723e27cf26d0414ba4015e186c29426c",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To reserve a table for another date, press '2'.",
                            "fileID": "4eec64f10fa74f5391839a5323a06935",
