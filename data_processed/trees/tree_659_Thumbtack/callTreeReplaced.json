{
    "type": "welcome",
    "contents": [
        {
            "text": "Thank you for calling Thumbtack, your San Francisco-based service connecting you with local professionals for any project.",
            "fileID": "04066c6d903548c78772fffcf33ebeba"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For help finding a professional, press 1.",
                "fileID": "86ea08cbd8b54a86994616ca56f6f4dd",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "For home improvement and repair services, press 1.",
                            "fileID": "b14dbbd6ebe14f2b92b25025782d88a2",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "All our home improvement experts are currently assisting other customers. Please hold the line, and we will connect you shortly.",
                                        "fileID": "62ce6226223f47329bf399bce1e07c71"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "9736776a379f496380242b82b7988e03",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "For cleaning and housekeeping services, press 2.",
                            "fileID": "7009b38023af48079b90e3bdbd02218c",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "All our cleaning specialists are busy at the moment. Please stay on the line, and we will be with you soon.",
