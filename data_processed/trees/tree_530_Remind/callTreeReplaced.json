    "contents": [
        {
            "text": "Welcome to Remind, the free messaging service from San Francisco that makes school communication simple.",
            "fileID": "642a11bee1f94ff2b26b01eacb0e5ed1"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
