{
    "type": "welcome",
    "contents": [
        {
            "text": "Welcome to Redbubble, the San Francisco based marketplace for awesome products designed by independent artists.",
            "fileID": "994f5273b89d4419ae4d8285338242f0"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "For questions about orders, shipping, and returns, press 1.",
                "fileID": "519e2196d66149b294bd1e7fc47bdfd5",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To track an existing order, press 1.",
                            "fileID": "d66143bea6c24c508040b96d3ab30b82",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to our order tracking specialist.",
                                        "fileID": "11590c093bbc425f84dee08149e6fc9d"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "d5dfd8b5ae2e46569a1101e5be4a9101",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To initiate a return or report a damaged item, press 2.",
                            "fileID": "d82e05700ebe420b965a535949c28678",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "One moment please, connecting you to our returns department.",
                                        "fileID": "4f94772f979449919071a3ffd1871645"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "9c8dd679f41745e6b64293ea05878e6b",
