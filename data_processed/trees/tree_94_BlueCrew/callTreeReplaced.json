{
    "type": "welcome",
    "contents": [
        {
            "text": "Welcome to BlueCrew, the San Francisco-based staffing platform that connects you to job opportunities straight from your phone.",
            "fileID": "b54dde2f7d8d4a35a3bd532a2022d02c"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are a worker looking for shifts, press '1'.",
                "fileID": "f45aa7f0e7224b7d95a647910777e43e",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To create or update your profile and start receiving shift offers, press '1'.",
                            "fileID": "0ecbe95228344dd686e7763e65e5bbeb",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to our team to assist with your profile setup.",
                                        "fileID": "3defc47e3dca478cbdd3646d51742877"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "2672432037e640a69b28ff3a08183489",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To speak with a support specialist about your account, press '2'.",
                            "fileID": "9a7f3562a59749ff898ee81e360005ab",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to a support specialist.",
                                        "fileID": "e0229d56f6b647648710a207cbd57c6a"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "e51c9f0676194cb89e333647997842c9",
                                "navigationGoals": null
                            }
                        }
                    ],
                    "nextSegment": null,
                    "id": "675d6cb486e44b479ff482b254b30204",
                    "navigationGoals": null
                }
            },
            {
                "text": "If you are an employer in need of staff, press '2'.",
                "fileID": "7fa4792554044f4ea780d8594af4f77f",
                "index": 2,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To post a new job or shift, press '1'.",
                            "fileID": "3fa52b915db94d8a9a7d65c39c08c8e1",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to our employer services team to post your job.",
                                        "fileID": "b2d3bbbc7003437dbc5888ee8ccfa541"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "1dc46dc7053f485488bd9a78326ddeab",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To speak with our sales team about staffing solutions, press '2'.",
                            "fileID": "b63ea45b05b049c8bf94fa308ea1c653",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
