        {
            "text": "Welcome to The League, San Francisco\u2019s premier selective dating app matching you with like-minded singles.",
            "fileID": "12aafd72100143a2a9199088ee470298"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are an existing League member, press 1.",
                "fileID": "1ca92dbcbb30408db848d29340a76736",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To update your profile or preferences, press 1.",
                            "fileID": "46861c265f494f508aeb5f0d3315df04",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you to a team member who will help you update your profile.",
