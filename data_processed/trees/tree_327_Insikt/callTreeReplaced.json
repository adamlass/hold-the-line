                "index": 2,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "For questions about payments, billing, or setting up autopay, press 1.",
                            "fileID": "2478b7c3230b4c6880be680d3fb43967",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
