    "contents": [
        {
            "text": "Thank you for calling ClearMetal, the San Francisco\u00ad-based AI\u00ad-powered logistics platform offering end-to-end, order-level shipment visibility.",
            "fileID": "48160c2f1d1a488499b9c9cd2a6965d5"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
