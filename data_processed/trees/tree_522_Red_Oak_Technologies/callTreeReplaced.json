    "contents": [
        {
            "text": "Thank you for calling Red Oak Technologies, South Bay\u2019s premier recruiting agency serving companies from startups to Fortune 500s.",
            "fileID": "aa432e7038bf490daf16b13dea80beb5"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you are a job seeker looking for new career opportunities, press 1.",
                "fileID": "7e4f22876a0b4b9f834edc16d25d3c05",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To speak directly with a recruiter about current openings, press 1.",
                            "fileID": "cb229da40d7c4725aa8577eb12837792",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "All of our recruiters are currently assisting other candidates. Please hold and we will connect you as soon as possible.",
