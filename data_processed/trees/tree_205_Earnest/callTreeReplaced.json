{
    "type": "welcome",
    "contents": [
        {
            "text": "Welcome to Earnest, where we combine innovative technology and personalized service to bring low interest loans to high potential people like you.",
            "fileID": "c343dae3fd874587933e0fc3b764b9f4"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
                "text": "If you\u2019re an existing Earnest loan customer, press 1.",
                "fileID": "9481713b63ce4efd9247d2259dce58a6",
                "index": 1,
                "nextSegment": {
                    "type": "dialOptions",
                    "contents": [
                        {
                            "text": "To check your loan balance and due date, press 1.",
                            "fileID": "e4771860a5b74291b5b0211d7e6acbeb",
                            "index": 1,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we retrieve your account balance information.",
                                        "fileID": "516ce0bef88f453e8cd3ea1b2deeca5e"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "153c9981e5b24239b9d878dc7ac6cc45",
                                "navigationGoals": null
                            }
                        },
                        {
                            "text": "To make a payment or discuss payment options, press 2.",
                            "fileID": "f99d62bc87d04f7189b24a913a20339a",
                            "index": 2,
                            "nextSegment": {
                                "type": "holdTheLine",
                                "contents": [
                                    {
                                        "text": "Please hold while we connect you with our payment services team.",
                                        "fileID": "15f105a371714620aec5a5af521922ae"
                                    }
                                ],
                                "nextSegment": null,
                                "id": "5baf48460dcb44339d735c78d7c9511f",
