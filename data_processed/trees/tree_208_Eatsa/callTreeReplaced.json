    "contents": [
        {
            "text": "Welcome to Eatsa in San Francisco, where we deliver a magical dining experience faster and more efficiently.",
            "fileID": "46338f89f8094678a6bbd608cb77345a"
        }
    ],
    "nextSegment": {
        "type": "dialOptions",
        "contents": [
            {
